@prefix odrl: <http://www.w3.org/ns/odrl/2#> .
@prefix odrlp: <http://example.org/odrl:profile:01#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <http://example.org#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/#> .
@prefix cert: <http://www.w3.org/ns/auth/cert#> .
@prefix spl: </langs/usage-policy#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .


spl:Authorization rdf:type owl:Class ;
    owl:equivalentClass 
[
spl:hasRecipient
    [ rdf:type ex:User ;
      ex:worksIn
          [ rdf:type foaf:Organization ;
            cert:key "https://example-organization-ABC.com" ;
            ex:offers "https://example-membership-ABC.com"
          ] ;
      ex:isMemberOf "https://example-membership-ABC.com" ;
      ex:isPartOf
          [ rdf:type odrlp:Subscription ;
            ex:hasSubscriber "https://example-organization-ABC.com"
          ]
    ] ;
spl:hasProcessing ex:grantAccess ;
spl:hasData [a ex:Session ]

 ] .